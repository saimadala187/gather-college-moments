<%- include('partials/header') %>

<div class="jumbotron text-center bg-change">
  <div class="container">
    <i class="fas fa-key fa-6x"></i>
    <h1 class="display-3">Memorable College Antics</h1>

<% usersWithSecrets.forEach(function(user){ %>
<% var dg=user.secret%>
  <%  dg.forEach(function(userSecret){ %>
    <p class="secret-text"><%=userSecret %></p>
<%  } )%>
<% }) %>

    <hr>

    <a class="btn btn-light btn-lg" href="/logout" role="button">Log Out</a>
    <a class="btn btn-dark btn-lg" href="/submit" role="button">Share a Remarkable Memory</a>
  </div>
</div>

<%- include('partials/footer') %>
